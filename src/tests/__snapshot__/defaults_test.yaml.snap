should render default Deployment:
  1: |
    replicas: 1
    revisionHistoryLimit: 3
    selector:
      matchLabels:
        app.kubernetes.io/controller: main
        app.kubernetes.io/instance: my-release
        app.kubernetes.io/name: workspace
    strategy:
      type: Recreate
    template:
      metadata:
        labels:
          app.kubernetes.io/controller: main
          app.kubernetes.io/instance: my-release
          app.kubernetes.io/name: workspace
      spec:
        automountServiceAccountToken: true
        containers:
          - image: ghcr.io/kloudkit/workspace:v0.1.2
            imagePullPolicy: IfNotPresent
            livenessProbe:
              failureThreshold: 3
              httpGet:
                path: /healthz
                port: 8080
                scheme: HTTP
              initialDelaySeconds: 0
              periodSeconds: 10
              timeoutSeconds: 1
            name: main
            readinessProbe:
              failureThreshold: 3
              httpGet:
                path: /healthz
                port: 8080
                scheme: HTTP
              initialDelaySeconds: 0
              periodSeconds: 10
              timeoutSeconds: 1
            volumeMounts:
              - mountPath: /workspace
                name: workspace
                subPath: workspace
              - mountPath: /home/kloud/.cache/history
                name: workspace
                subPath: history
        dnsPolicy: ClusterFirst
        enableServiceLinks: false
        hostIPC: false
        hostNetwork: false
        hostPID: false
        serviceAccountName: default
        volumes:
          - name: workspace
            persistentVolumeClaim:
              claimName: my-release-workspace
should render default PVC:
  1: |
    accessModes:
      - ReadWriteOnce
    resources:
      requests:
        storage: 25Gi
should render default Service:
  1: |
    ports:
      - name: http
        port: 8080
        protocol: TCP
        targetPort: 8080
    selector:
      app.kubernetes.io/controller: main
      app.kubernetes.io/instance: my-release
      app.kubernetes.io/name: workspace
    type: ClusterIP
